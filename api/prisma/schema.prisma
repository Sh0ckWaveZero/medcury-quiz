generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:dev.db"
}

model User {
  id        Int      @id @default(autoincrement())
  telephone String   @unique
  name      String?
  pin_code  Int?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
